   "outputs": [],
   "source": [
    "path = \"/Users/pei-yu/Desktop/chromedriver\"\n",
    "deiver = webdriver.Chrome(path)\n",
    "deiver.get(\"https://popcat.click\")\n",
    "cat = deiver.find_element_by_xpath('//*[@id=\"app\"]/div')\n",
    "\n",
    "while True:\n",
    "    cat.click()\n",
    "    sleep(0.3)"
